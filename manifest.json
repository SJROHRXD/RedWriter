{
    "manifest_version": 3,
    "name": "RedWriter - SJRXD",
    "description": "Extract and Copy YouTube Video Transcripts",
    "version": "1.0",
    "permissions": [
        "tabs",
        "activeTab",
        "scripting"
        ],
    "host_permissions": [
            "https://www.youtube.com/*"],
    "action": {
        "default_popup": "redwriter.html",
        "default_icon": "RedWriterQuickICO.png"
    }
}